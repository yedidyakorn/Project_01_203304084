package primitives;

public class Material {

    private double kD;
    private double kS;
    private  int nShininess;

    public Material(double d, double s, int n){

        this.kD = d;
        this.kS = s;
        this.nShininess = n;

    }

    public  double getkD(){
        return kD;
    }

    public  double getkS(){
        return kS;
    }

    public  int getnShininess(){
        return  nShininess;
    }


}
